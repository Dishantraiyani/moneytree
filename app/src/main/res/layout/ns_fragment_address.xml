<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	tools:context=".ui.profile.password.NSChangePasswordFragment">

	<include android:id="@+id/layout_header"
		layout="@layout/layout_header"/>

	<androidx.core.widget.NestedScrollView
		style="@style/LayoutWidthHeightZero"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintTop_toBottomOf="@id/layout_header"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintEnd_toEndOf="parent">

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="vertical"
			android:paddingVertical="@dimen/_20sdp"
			android:paddingHorizontal="@dimen/_15sdp">

			<TextView
				style="@style/LoginSubFontsWrap"
				android:layout_height="wrap_content"
				android:layout_marginStart="@dimen/_5sdp"
				android:text="@string/full_name"
				android:textColor="@color/black"
				android:textSize="@dimen/_10sdp" />

			<androidx.cardview.widget.CardView
				android:id="@+id/card_full_name"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="@dimen/_6sdp"
				app:cardCornerRadius="@dimen/_5sdp"
				android:layout_marginHorizontal="@dimen/_5sdp"
				app:layout_constraintStart_toStartOf="parent"
				app:layout_constraintEnd_toEndOf="parent"
				app:layout_constraintTop_toBottomOf="@+id/card_wallet_amount">

				<androidx.constraintlayout.widget.ConstraintLayout
					android:id="@+id/cl_full_name"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="@dimen/_1sdp"
					app:guidelineUseRtl="true"
					app:chainUseRtl="true"
					app:layout_constraintStart_toStartOf="parent"
					app:layout_constraintEnd_toEndOf="parent"
					app:layout_constraintTop_toBottomOf="@id/tv_login_sub_title">

					<ImageView
						android:id="@+id/iv_member_name"
						android:layout_width="@dimen/_15sdp"
						android:layout_height="@dimen/_15sdp"
						android:src="@drawable/tab_profile"
						android:layout_marginStart="@dimen/_10sdp"
						android:visibility="gone"
						app:layout_constraintStart_toStartOf="parent"
						app:layout_constraintTop_toTopOf="parent"
						app:layout_constraintBottom_toBottomOf="parent"
						android:contentDescription="@string/username_title" />

					<EditText
						android:id="@+id/et_full_name"
						style="@style/LoginSubFontsZeroWrap"
						android:minHeight="@dimen/_38sdp"
						android:textColor="@color/black"
						android:lines="1"
						android:background="@null"
						android:inputType="textPersonName"
						android:paddingHorizontal="@dimen/_10sdp"
						android:hint="@string/full_name"
						android:textColorHint="@color/hint_color"
						app:layout_constraintTop_toTopOf="parent"
						app:layout_constraintBottom_toBottomOf="parent"
						app:layout_constraintStart_toEndOf="@id/iv_member_name"
						app:layout_constraintEnd_toEndOf="parent"
						android:gravity="center_vertical"/>

				</androidx.constraintlayout.widget.ConstraintLayout>

			</androidx.cardview.widget.CardView>

			<TextView
				style="@style/LoginSubFontsWrap"
				android:layout_height="wrap_content"
				android:layout_marginStart="@dimen/_5sdp"
				android:layout_marginTop="@dimen/_15sdp"
				android:text="@string/mobile"
				android:textColor="@color/black"
				android:textSize="@dimen/_10sdp" />

			<androidx.cardview.widget.CardView
				android:id="@+id/card_mobile"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="@dimen/_6sdp"
				app:cardCornerRadius="@dimen/_5sdp"
				android:layout_marginHorizontal="@dimen/_5sdp"
				app:layout_constraintStart_toStartOf="parent"
				app:layout_constraintEnd_toEndOf="parent"
				app:layout_constraintTop_toBottomOf="@+id/card_wallet_amount">

				<androidx.constraintlayout.widget.ConstraintLayout
					android:id="@+id/cl_mobile"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="@dimen/_1sdp"
					app:guidelineUseRtl="true"
					app:chainUseRtl="true">

					<ImageView
						android:id="@+id/iv_mobile"
						android:layout_width="@dimen/_15sdp"
						android:layout_height="@dimen/_15sdp"
						android:src="@drawable/ic_phone"
						android:layout_marginStart="@dimen/_10sdp"
						android:visibility="gone"
						app:layout_constraintStart_toStartOf="parent"
						app:layout_constraintTop_toTopOf="parent"
						app:layout_constraintBottom_toBottomOf="parent"
						android:contentDescription="@string/username_title" />

					<EditText
						android:id="@+id/et_mobile"
						style="@style/LoginSubFontsZeroWrap"
						android:minHeight="@dimen/_38sdp"
						android:textColor="@color/black"
						android:lines="1"
						android:background="@null"
						android:paddingHorizontal="@dimen/_10sdp"
						android:hint="@string/mobile"
						android:inputType="phone"
						android:digits="0123456789"
						android:maxLength="10"
						android:textColorHint="@color/hint_color"
						app:layout_constraintTop_toTopOf="parent"
						app:layout_constraintBottom_toBottomOf="parent"
						app:layout_constraintStart_toEndOf="@id/iv_mobile"
						app:layout_constraintEnd_toEndOf="parent"
						android:gravity="center_vertical"/>

				</androidx.constraintlayout.widget.ConstraintLayout>

			</androidx.cardview.widget.CardView>

			<TextView
				style="@style/LoginSubFontsWrap"
				android:text="@string/flat_house"
				android:textColor="@color/black"
				android:layout_marginStart="@dimen/_5sdp"
				android:layout_marginTop="@dimen/_15sdp"
				android:textSize="@dimen/_10sdp"/>

			<androidx.cardview.widget.CardView
				android:id="@+id/card_recharge_type"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="@dimen/_6sdp"
				app:cardCornerRadius="@dimen/_5sdp"
				android:layout_marginHorizontal="@dimen/_5sdp">

				<androidx.constraintlayout.widget.ConstraintLayout
					android:id="@+id/cl_recharge_type"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="@dimen/_1sdp"
					app:guidelineUseRtl="true"
					app:chainUseRtl="true"
					app:layout_constraintStart_toStartOf="parent"
					app:layout_constraintEnd_toEndOf="parent"
					app:layout_constraintTop_toBottomOf="@id/tv_login_sub_title">

					<ImageView
						android:id="@+id/iv_recharge_type"
						android:layout_width="@dimen/_15sdp"
						android:layout_height="@dimen/_15sdp"
						android:src="@drawable/ic_wallet_coin"
						android:layout_marginStart="@dimen/_10sdp"
						android:visibility="gone"
						app:layout_constraintStart_toStartOf="parent"
						app:layout_constraintTop_toTopOf="parent"
						app:layout_constraintBottom_toBottomOf="parent"
						android:contentDescription="@string/username_title" />

					<EditText
						android:id="@+id/et_flat_house"
						style="@style/LoginSubFontsZeroWrap"
						android:minHeight="@dimen/_38sdp"
						android:textColor="@color/black"
						android:lines="1"
						android:background="@null"
						android:paddingHorizontal="@dimen/_10sdp"
						android:hint="@string/address"
						android:inputType="textPostalAddress"
						android:textColorHint="@color/hint_color"
						app:layout_constraintTop_toTopOf="parent"
						app:layout_constraintBottom_toBottomOf="parent"
						app:layout_constraintStart_toEndOf="@id/iv_recharge_type"
						app:layout_constraintEnd_toEndOf="parent"
						android:gravity="center_vertical"/>

				</androidx.constraintlayout.widget.ConstraintLayout>

			</androidx.cardview.widget.CardView>

			<TextView
				style="@style/LoginSubFontsWrap"
				android:text="@string/area"
				android:textColor="@color/black"
				android:layout_marginStart="@dimen/_5sdp"
				android:layout_marginTop="@dimen/_15sdp"
				android:textSize="@dimen/_10sdp"/>

			<androidx.cardview.widget.CardView
				android:id="@+id/card_area"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="@dimen/_6sdp"
				app:cardCornerRadius="@dimen/_5sdp"
				android:layout_marginHorizontal="@dimen/_5sdp"
				app:layout_constraintStart_toStartOf="parent"
				app:layout_constraintEnd_toEndOf="parent"
				app:layout_constraintTop_toTopOf="parent">

				<androidx.constraintlayout.widget.ConstraintLayout
					android:id="@+id/cl_area"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="@dimen/_1sdp"
					app:guidelineUseRtl="true"
					app:chainUseRtl="true"
					app:layout_constraintStart_toStartOf="parent"
					app:layout_constraintEnd_toEndOf="parent"
					app:layout_constraintTop_toBottomOf="@id/tv_login_sub_title">

					<ImageView
						android:id="@+id/iv_area"
						android:layout_width="@dimen/_15sdp"
						android:layout_height="@dimen/_15sdp"
						android:src="@drawable/remark"
						android:layout_marginStart="@dimen/_10sdp"
						android:visibility="gone"
						app:layout_constraintStart_toStartOf="parent"
						app:layout_constraintTop_toTopOf="parent"
						app:layout_constraintBottom_toBottomOf="parent"
						android:contentDescription="@string/username_title" />

					<EditText
						android:id="@+id/et_area"
						style="@style/LoginSubFontsZeroWrap"
						android:minHeight="@dimen/_38sdp"
						android:textColor="@color/black"
						android:inputType="text"
						android:lines="1"
						android:background="@drawable/phone_white_background"
						android:paddingHorizontal="@dimen/_10sdp"
						android:hint="@string/area"
						android:gravity="center_vertical"
						android:textColorHint="@color/hint_color"
						app:layout_constraintTop_toTopOf="parent"
						app:layout_constraintBottom_toBottomOf="parent"
						app:layout_constraintStart_toEndOf="@id/iv_area"
						app:layout_constraintEnd_toEndOf="parent" />

				</androidx.constraintlayout.widget.ConstraintLayout>

			</androidx.cardview.widget.CardView>

			<TextView
				style="@style/LoginSubFontsWrap"
				android:text="@string/landmark"
				android:textColor="@color/black"
				android:layout_marginStart="@dimen/_5sdp"
				android:layout_marginTop="@dimen/_15sdp"
				android:textSize="@dimen/_10sdp"/>

			<androidx.cardview.widget.CardView
				android:id="@+id/card_land_mark"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="@dimen/_6sdp"
				app:cardCornerRadius="@dimen/_5sdp"
				android:layout_marginHorizontal="@dimen/_5sdp"
				app:layout_constraintStart_toStartOf="parent"
				app:layout_constraintEnd_toEndOf="parent"
				app:layout_constraintTop_toTopOf="parent">

				<androidx.constraintlayout.widget.ConstraintLayout
					android:id="@+id/cl_land_mark"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="@dimen/_1sdp"
					app:guidelineUseRtl="true"
					app:chainUseRtl="true"
					app:layout_constraintStart_toStartOf="parent"
					app:layout_constraintEnd_toEndOf="parent"
					app:layout_constraintTop_toBottomOf="@id/tv_login_sub_title">

					<ImageView
						android:id="@+id/iv_land_mark"
						android:layout_width="@dimen/_15sdp"
						android:layout_height="@dimen/_15sdp"
						android:src="@drawable/remark"
						android:layout_marginStart="@dimen/_10sdp"
						android:visibility="gone"
						app:layout_constraintStart_toStartOf="parent"
						app:layout_constraintTop_toTopOf="parent"
						app:layout_constraintBottom_toBottomOf="parent"
						android:contentDescription="@string/username_title" />

					<EditText
						android:id="@+id/et_land_mark"
						style="@style/LoginSubFontsZeroWrap"
						android:minHeight="@dimen/_38sdp"
						android:textColor="@color/black"
						android:inputType="text"
						android:lines="1"
						android:background="@drawable/phone_white_background"
						android:paddingHorizontal="@dimen/_10sdp"
						android:hint="@string/landmark_hint"
						android:gravity="center_vertical"
						android:textColorHint="@color/hint_color"
						app:layout_constraintTop_toTopOf="parent"
						app:layout_constraintBottom_toBottomOf="parent"
						app:layout_constraintStart_toEndOf="@id/iv_land_mark"
						app:layout_constraintEnd_toEndOf="parent" />

				</androidx.constraintlayout.widget.ConstraintLayout>

			</androidx.cardview.widget.CardView>

			<TextView
				style="@style/LoginSubFontsWrap"
				android:text="@string/pin_code"
				android:textColor="@color/black"
				android:layout_marginStart="@dimen/_5sdp"
				android:layout_marginTop="@dimen/_15sdp"
				android:textSize="@dimen/_10sdp"/>

			<androidx.cardview.widget.CardView
				android:id="@+id/card_pin_code"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="@dimen/_6sdp"
				app:cardCornerRadius="@dimen/_5sdp"
				android:layout_marginHorizontal="@dimen/_5sdp"
				app:layout_constraintStart_toStartOf="parent"
				app:layout_constraintEnd_toEndOf="parent"
				app:layout_constraintTop_toTopOf="parent">

				<androidx.constraintlayout.widget.ConstraintLayout
					android:id="@+id/cl_pin_code"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="@dimen/_1sdp"
					app:guidelineUseRtl="true"
					app:chainUseRtl="true"
					app:layout_constraintStart_toStartOf="parent"
					app:layout_constraintEnd_toEndOf="parent"
					app:layout_constraintTop_toBottomOf="@id/tv_login_sub_title">

					<ImageView
						android:id="@+id/iv_pin_code"
						android:layout_width="@dimen/_15sdp"
						android:layout_height="@dimen/_15sdp"
						android:src="@drawable/remark"
						android:layout_marginStart="@dimen/_10sdp"
						android:visibility="gone"
						app:layout_constraintStart_toStartOf="parent"
						app:layout_constraintTop_toTopOf="parent"
						app:layout_constraintBottom_toBottomOf="parent"
						android:contentDescription="@string/username_title" />

					<EditText
						android:id="@+id/et_pin_code"
						style="@style/LoginSubFontsZeroWrap"
						android:minHeight="@dimen/_38sdp"
						android:textColor="@color/black"
						android:inputType="number"
						android:lines="1"
						android:background="@drawable/phone_white_background"
						android:paddingHorizontal="@dimen/_10sdp"
						android:hint="@string/pin_code"
						android:digits="0123456789"
						android:gravity="center_vertical"
						android:textColorHint="@color/hint_color"
						app:layout_constraintTop_toTopOf="parent"
						app:layout_constraintBottom_toBottomOf="parent"
						app:layout_constraintStart_toEndOf="@id/iv_pin_code"
						app:layout_constraintEnd_toEndOf="parent" />

				</androidx.constraintlayout.widget.ConstraintLayout>

			</androidx.cardview.widget.CardView>

			<TextView
				style="@style/LoginSubFontsWrap"
				android:text="@string/country"
				android:textColor="@color/black"
				android:layout_marginTop="@dimen/_15sdp"
				android:layout_marginStart="@dimen/_5sdp"
				android:textSize="@dimen/_10sdp"/>

			<androidx.cardview.widget.CardView
				android:id="@+id/card_country"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="@dimen/_6sdp"
				app:cardCornerRadius="@dimen/_5sdp"
				android:layout_marginHorizontal="@dimen/_5sdp"
				app:layout_constraintStart_toStartOf="parent"
				app:layout_constraintEnd_toEndOf="parent"
				app:layout_constraintTop_toTopOf="parent">

				<androidx.constraintlayout.widget.ConstraintLayout
					android:id="@+id/cl_country"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="@dimen/_1sdp"
					app:guidelineUseRtl="true"
					app:chainUseRtl="true"
					app:layout_constraintStart_toStartOf="parent"
					app:layout_constraintEnd_toEndOf="parent"
					app:layout_constraintTop_toBottomOf="@id/tv_login_sub_title">

					<ImageView
						android:id="@+id/iv_location"
						android:layout_width="@dimen/_15sdp"
						android:layout_height="@dimen/_15sdp"
						android:visibility="gone"
						android:src="@drawable/tab_profile"
						android:layout_marginStart="@dimen/_10sdp"
						app:layout_constraintStart_toStartOf="parent"
						app:layout_constraintTop_toTopOf="parent"
						app:layout_constraintBottom_toBottomOf="parent"
						android:contentDescription="@string/username_title" />

					<EditText
						android:id="@+id/et_country_name"
						style="@style/LoginSubFontsZeroWrap"
						android:minHeight="@dimen/_38sdp"
						android:textColor="@color/black"
						android:lines="1"
						android:background="@null"
						android:inputType="textPersonName"
						android:paddingHorizontal="@dimen/_10sdp"
						android:hint="@string/country"
						android:textColorHint="@color/hint_color"
						app:layout_constraintTop_toTopOf="parent"
						app:layout_constraintBottom_toBottomOf="parent"
						app:layout_constraintStart_toEndOf="@id/iv_location"
						app:layout_constraintEnd_toEndOf="parent"
						android:gravity="center_vertical"/>

				</androidx.constraintlayout.widget.ConstraintLayout>

			</androidx.cardview.widget.CardView>

			<TextView
				style="@style/LoginSubFontsWrap"
				android:text="@string/state"
				android:textColor="@color/black"
				android:layout_marginStart="@dimen/_5sdp"
				android:layout_marginTop="@dimen/_15sdp"
				android:textSize="@dimen/_10sdp"/>

			<androidx.cardview.widget.CardView
				android:id="@+id/card_state"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="@dimen/_6sdp"
				app:cardCornerRadius="@dimen/_5sdp"
				android:layout_marginHorizontal="@dimen/_5sdp"
				app:layout_constraintStart_toStartOf="parent"
				app:layout_constraintEnd_toEndOf="parent"
				app:layout_constraintTop_toTopOf="parent">

				<androidx.constraintlayout.widget.ConstraintLayout
					android:id="@+id/cl_state"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="@dimen/_1sdp"
					app:guidelineUseRtl="true"
					app:chainUseRtl="true"
					app:layout_constraintStart_toStartOf="parent"
					app:layout_constraintEnd_toEndOf="parent"
					app:layout_constraintTop_toBottomOf="@id/tv_login_sub_title">

					<ImageView
						android:id="@+id/iv_state"
						android:layout_width="@dimen/_15sdp"
						android:layout_height="@dimen/_15sdp"
						android:src="@drawable/remark"
						android:layout_marginStart="@dimen/_10sdp"
						android:visibility="gone"
						app:layout_constraintStart_toStartOf="parent"
						app:layout_constraintTop_toTopOf="parent"
						app:layout_constraintBottom_toBottomOf="parent"
						android:contentDescription="@string/username_title" />

					<EditText
						android:id="@+id/et_state"
						style="@style/LoginSubFontsZeroWrap"
						android:minHeight="@dimen/_38sdp"
						android:textColor="@color/black"
						android:inputType="text"
						android:lines="1"
						android:background="@drawable/phone_white_background"
						android:paddingHorizontal="@dimen/_10sdp"
						android:hint="@string/state"
						android:gravity="center_vertical"
						android:textColorHint="@color/hint_color"
						app:layout_constraintTop_toTopOf="parent"
						app:layout_constraintBottom_toBottomOf="parent"
						app:layout_constraintStart_toEndOf="@id/iv_state"
						app:layout_constraintEnd_toEndOf="parent" />

				</androidx.constraintlayout.widget.ConstraintLayout>

			</androidx.cardview.widget.CardView>


			<TextView
				style="@style/LoginSubFontsWrap"
				android:text="@string/town"
				android:textColor="@color/black"
				android:layout_marginStart="@dimen/_5sdp"
				android:layout_marginTop="@dimen/_15sdp"
				android:textSize="@dimen/_10sdp"/>

			<androidx.cardview.widget.CardView
				android:id="@+id/card_city"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="@dimen/_6sdp"
				app:cardCornerRadius="@dimen/_5sdp"
				android:layout_marginHorizontal="@dimen/_5sdp"
				app:layout_constraintStart_toStartOf="parent"
				app:layout_constraintEnd_toEndOf="parent"
				app:layout_constraintTop_toTopOf="parent">

				<androidx.constraintlayout.widget.ConstraintLayout
					android:id="@+id/cl_city"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="@dimen/_1sdp"
					app:guidelineUseRtl="true"
					app:chainUseRtl="true"
					app:layout_constraintStart_toStartOf="parent"
					app:layout_constraintEnd_toEndOf="parent"
					app:layout_constraintTop_toBottomOf="@id/tv_login_sub_title">

					<ImageView
						android:id="@+id/iv_city"
						android:layout_width="@dimen/_15sdp"
						android:layout_height="@dimen/_15sdp"
						android:src="@drawable/remark"
						android:layout_marginStart="@dimen/_10sdp"
						android:visibility="gone"
						app:layout_constraintStart_toStartOf="parent"
						app:layout_constraintTop_toTopOf="parent"
						app:layout_constraintBottom_toBottomOf="parent"
						android:contentDescription="@string/username_title" />

					<EditText
						android:id="@+id/et_city"
						style="@style/LoginSubFontsZeroWrap"
						android:minHeight="@dimen/_38sdp"
						android:textColor="@color/black"
						android:inputType="text"
						android:lines="1"
						android:background="@drawable/phone_white_background"
						android:paddingHorizontal="@dimen/_10sdp"
						android:hint="@string/town"
						android:gravity="center_vertical"
						android:textColorHint="@color/hint_color"
						app:layout_constraintTop_toTopOf="parent"
						app:layout_constraintBottom_toBottomOf="parent"
						app:layout_constraintStart_toEndOf="@id/iv_city"
						app:layout_constraintEnd_toEndOf="parent" />

				</androidx.constraintlayout.widget.ConstraintLayout>

			</androidx.cardview.widget.CardView>

			<LinearLayout
				android:id="@+id/ll_check"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="horizontal"
				android:layout_marginTop="@dimen/_20sdp"
				app:layout_constraintStart_toStartOf="parent"
				app:layout_constraintEnd_toEndOf="parent"
				app:layout_constraintTop_toBottomOf="@id/card_confirm_password">

				<CheckBox
					android:id="@+id/cb_checked"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:buttonTint="@color/orange"
					/>

				<TextView
					android:id="@+id/tv_terms_conditions"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_gravity="center_vertical"
					android:paddingStart="@dimen/_5sdp"
					android:text="@string/make_this_my_default_address"
					android:textColorLink="@color/orange"
					android:paddingEnd="@dimen/_15sdp"
					android:lineSpacingExtra="@dimen/_2sdp"
					android:textColor="@color/black"
					android:fontFamily="@font/inter_regular"/>

			</LinearLayout>

			<Button
