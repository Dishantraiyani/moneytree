<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.cardview.widget.CardView
        android:id="@+id/card_meeting"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_5sdp"
        android:layout_marginBottom="@dimen/_5sdp"
        app:cardCornerRadius="@dimen/_5sdp"
        android:layout_marginHorizontal="@dimen/_10sdp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_member_id"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="@dimen/_10sdp"
            app:guidelineUseRtl="true"
            app:chainUseRtl="true"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tv_login_sub_title">

            <TextView
                android:id="@+id/et_meeting_name"
                style="@style/LoginSubFontsZeroWrap"
                android:textColor="@color/black"
                android:lines="1"
                tools:text="Meeting Name"
                android:background="@drawable/phone_white_background"
                android:gravity="center_vertical"
                android:textStyle="bold"
                android:textSize="@dimen/_12sdp"
                android:textColorHint="@color/hint_color"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/iv_delete" />

            <View
                android:id="@+id/view_line_divider"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1sdp"
                android:background="@color/line_color"
                android:layout_marginTop="@dimen/_10sdp"
                app:layout_constraintTop_toBottomOf="@id/et_meeting_name"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"/>

            <TextView
                android:id="@+id/tv_host_name_title"
                style="@style/LoginSubFontsWrap"
                android:text="@string/host_name"
                android:textColor="@color/hint_color"
                android:minWidth="@dimen/_100sdp"
                android:background="@drawable/phone_white_background"
                android:gravity="center_vertical"
                android:textSize="@dimen/_10sdp"
                android:paddingVertical="0dp"
                android:layout_marginTop="@dimen/_8sdp"
                android:textColorHint="@color/hint_color"
                app:layout_constraintTop_toBottomOf="@+id/view_line_divider"
                app:layout_constraintStart_toStartOf="parent"
                />

            <TextView
                android:id="@+id/tv_host_name"
                style="@style/LoginSubFontsZeroWrap"
                tools:text="@string/meeting_date"
                android:textColor="@color/black"
                android:background="@drawable/phone_white_background"
                android:gravity="center_vertical|end"
                android:textSize="@dimen/_10sdp"
                android:paddingVertical="0dp"
                android:textColorHint="@color/hint_color"
                app:layout_constraintStart_toEndOf="@id/tv_host_name_title"
                app:layout_constraintTop_toTopOf="@id/tv_host_name_title"
                app:layout_constraintBottom_toBottomOf="@id/tv_host_name_title"
                app:layout_constraintEnd_toEndOf="parent" />

            <TextView
                android:id="@+id/tv_special_guest_name_title"
                style="@style/LoginSubFontsWrap"
                android:text="@string/special_guest_name"
                android:textColor="@color/hint_color"
                android:minWidth="@dimen/_100sdp"
                android:background="@drawable/phone_white_background"
                android:gravity="center_vertical"
                android:textSize="@dimen/_10sdp"
                android:paddingVertical="0dp"
                android:layout_marginTop="@dimen/_8sdp"
                android:textColorHint="@color/hint_color"
                app:layout_constraintTop_toBottomOf="@+id/tv_host_name_title"
                app:layout_constraintStart_toStartOf="parent"
                />

            <TextView
                android:id="@+id/tv_special_guest_name"
                style="@style/LoginSubFontsZeroWrap"
                tools:text="@string/meeting_date"
                android:textColor="@color/black"
                android:background="@drawable/phone_white_background"
                android:gravity="center_vertical|end"
                android:textSize="@dimen/_10sdp"
                android:paddingVertical="0dp"
                android:textColorHint="@color/hint_color"
                app:layout_constraintStart_toEndOf="@id/tv_special_guest_name_title"
                app:layout_constraintTop_toTopOf="@id/tv_special_guest_name_title"
                app:layout_constraintBottom_toBottomOf="@id/tv_special_guest_name_title"
                app:layout_constraintEnd_toEndOf="parent" />

            <TextView
                android:id="@+id/tv_meeting_date_title"
                style="@style/LoginSubFontsZeroWrap"
                android:text="@string/meeting_date"
                android:textColor="@color/hint_color"
                android:minWidth="@dimen/_100sdp"
                android:background="@drawable/phone_white_background"
                android:gravity="center_vertical"
                android:textSize="@dimen/_10sdp"
                android:paddingVertical="0dp"
                android:layout_marginTop="@dimen/_8sdp"
                android:textColorHint="@color/hint_color"
                app:layout_constraintTop_toBottomOf="@+id/tv_special_guest_name_title"
                app:layout_constraintStart_toStartOf="parent"
                />

            <TextView
                android:id="@+id/tv_meeting_date"
                style="@style/LoginSubFontsZeroWrap"
                tools:text="@string/meeting_date"
                android:textColor="@color/black"
                android:background="@drawable/phone_white_background"
                android:gravity="center_vertical|end"
                android:textSize="@dimen/_10sdp"
                android:paddingVertical="0dp"
                android:textColorHint="@color/hint_color"
                app:layout_constraintStart_toEndOf="@id/tv_meeting_date_title"
                app:layout_constraintTop_toTopOf="@id/tv_meeting_date_title"
                app:layout_constraintBottom_toBottomOf="@id/tv_meeting_date_title"
                app:layout_constraintEnd_toEndOf="parent" />

            <TextView
                android:id="@+id/tv_meeting_time_title"
                style="@style/LoginSubFontsZeroWrap"
                android:text="@string/meeting_time"
                android:textColor="@color/hint_color"
                android:minWidth="@dimen/_100sdp"
                android:background="@drawable/phone_white_background"
                android:gravity="center_vertical"
                android:textSize="@dimen/_10sdp"
                android:paddingVertical="0dp"
                android:layout_marginTop="@dimen/_8sdp"
                android:textColorHint="@color/hint_color"
                app:layout_constraintTop_toBottomOf="@+id/tv_meeting_date_title"
                app:layout_constraintStart_toStartOf="parent"
                />

            <TextView
                android:id="@+id/tv_meeting_time"
                style="@style/LoginSubFontsZeroWrap"
                tools:text="@string/meeting_date"
                android:textColor="@color/orange"
                android:background="@drawable/phone_white_background"
                android:gravity="center_vertical|end"
                android:textSize="@dimen/_10sdp"
                android:paddingVertical="0dp"
                android:textColorHint="@color/hint_color"
                app:layout_constraintStart_toEndOf="@id/tv_meeting_time_title"
                app:layout_constraintTop_toTopOf="@id/tv_meeting_time_title"
                app:layout_constraintBottom_toBottomOf="@id/tv_meeting_time_title"
                app:layout_constraintEnd_toEndOf="parent" />

            <TextView
                android:id="@+id/tv_address_title"
                style="@style/LoginSubFontsWrap"
                android:text="@string/address"
                android:textColor="@color/hint_color"
                android:minWidth="@dimen/_100sdp"
                android:background="@drawable/phone_white_background"
                android:gravity="center_vertical"
                android:textSize="@dimen/_10sdp"
                android:paddingVertical="0dp"
                android:layout_marginTop="@dimen/_8sdp"
                android:textColorHint="@color/hint_color"
                app:layout_constraintTop_toBottomOf="@+id/tv_meeting_time_title"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/tv_address"
                style="@style/LoginSubFontsZeroWrap"
                tools:text="aasdadadasdasd\nasdasd"
                android:textColor="@color/black"
                android:background="@drawable/phone_white_background"
                android:gravity="center_vertical|end"
                android:textSize="@dimen/_10sdp"
                android:paddingVertical="0dp"
                android:textColorHint="@color/hint_color"
                app:layout_constraintTop_toTopOf="@id/tv_address_title"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@id/tv_address_title"/>

            <ImageView
                android:id="@+id/iv_delete"
                android:layout_width="@dimen/_15sdp"
                android:layout_height="@dimen/_15sdp"
                android:src="@drawable/ic_trash"
                android:tint="@color/red"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/et_meeting_name"
                app:layout_constraintBottom_toBottomOf="@+id/et_meeting_name"
                android:contentDescription="@string/username_title"
                tools:ignore="UseAppTint" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>